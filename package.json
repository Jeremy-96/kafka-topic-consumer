{
  "name": "kafka-topic-consumer",
  "version": "1.0.0",
  "main": "src/app.ts",
  "scripts": {
    "start": "tsc && node scripts/replace-extension.js && node dist/app.js",
    "start:kafka": "tsc && node scripts/replace-extension.js && node dist/app-kafka.js",
    "build": "tsc && node scripts/replace-extension.js",
    "lint": "prettier --check && eslint . --ext .ts",
    "lint:fix": "prettier --check && eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "migration:generate": "typeorm-ts-node-module -d src/database/data-source.ts migration:generate src/database/migrations/default",
    "migration:run": "typeorm-ts-node-module -d src/database/data-source.ts migration:run"
  },
  "author": "Jérémy Thonon <jeremythonon96@hotmail.com>",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "kafkajs": "^2.2.4",
    "kafkajs-lz4": "1.2.1",
    "lz4": "^0.6.5",
    "lz4-asm": "^0.4.2",
    "lz4-kafkajs": "^1.0.0",
    "nodemon": "^3.1.10",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.22"
  },
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@tsconfig/node20": "^20.1.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.1",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.3.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "replace-in-file": "^8.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  }
}
